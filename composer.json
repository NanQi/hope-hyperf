{
    "name": "nanqi/hope-hyperf",
    "type": "library",
    "description": "Hyperf基础API支持",
    "keywords": ["hyperf", "hope", "api"],
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "NanQi",
            "email": "luyu_1216@126.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "hyperf/di": "~2.0.0",
        "hyperf/framework": "~2.0.0",
        "hyperf/contract": "~2.0.0",
        "hyperf/redis": "~2.0.0",
        "hyperf/constants": "^2.0",
        "hyperf/validation": "^2.0",
        "hyperf/circuit-breaker": "^2.0",
        "hyperf/db-connection": "~2.0.0",
        "lcobucci/jwt": "^3.3",
        "hyperf/cache": "~2.0.0",
        "hyperf/async-queue": "^2.0",
        "ext-json": "*",
        "ext-redis": "*"
    },
    "require-dev": {
        "ext-json": "*",
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "^5",
        "filp/whoops": "^2.7",
        "fideloper/proxy": "^4.2",
        "fzaninotto/faker": "^1.9"
    },
    "extra": {
        "hyperf": {
            "config": "NanQi\\Hope\\ConfigProvider"
        }
    },
    "autoload": {
        "psr-4": {
            "NanQi\\Hope\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    }
}
